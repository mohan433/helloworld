pipeline {
    agent any

    Stages {
       stages ('Compile Stages') {

          steps {
             withMaven(maven : 'apache-maven-3.6.3') }
               sh 'mvn clean compile'
             }
          }
       }
       stage ('testing Satge') {
          steps {
             withMaven(maven : 'apache-maven-3.6.3') }
                sh 'mvn test'
                sh 'https://github.com/mohan433/helloworld.git'
             }
          }
       }
       stage ('Deployment Satge') {
          steps {
             withMaven(maven : 'apache-maven-3.6.3') }
               sh 'mvn deployment'
             }
          }
       }
    }
 }
